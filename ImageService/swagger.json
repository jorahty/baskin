{
  "swagger": "2.0",
  "info": {
    "title": "ImageService",
    "version": "1.0"
  },
  "paths": {
    "/images/{fileName}": {
      "get": {
        "parameters": [
          {
            "type": "string",
            "name": "fileName",
            "in": "path",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/images": {
      "post": {
        "summary": "Uploads a new JPEG image",
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "images",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "fileName": {
                  "type": "string",
                  "example": "kitten.jpg"
                },
                "imageData": {
                  "type": "string"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "fileName",
            "schema": {
              "type": "string",
              "example": "kitten.jpg"
            }
          }
        }
      }
    }
  }
}