{
  "entryFile": "src/app.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": [
    "src/**/*Controller.ts"
  ],
  "spec": {
    "outputDirectory": "build",
    "specVersion": 3,
    "schemes": [
      "http"
    ],
    "basePath": "/api/v0",
    "specMerging": "recursive",
    "spec": {
      "paths": {
        "/images": {
          "post": {
            "consumes": [
              "multipart/form-data"
            ],
            "requestBody": {
              "content": {
                "multipart/form-data": {
                  "schema": {
                    "properties": {
                      "file": {
                        "type": "string",
                        "format": "binary"
                      }
                    },
                    "required": [
                      "file"
                    ],
                    "type": "object"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "routes": {
    "routesDir": "build"
  }
}
