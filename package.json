{
  "name": "baskin",
  "scripts": {
    "docker": "npm run docker-user && npm run docker-account",
    "docker-user": "cd UserApp && docker-compose up -d",
    "docker-account": "cd AccountService && docker-compose up -d",
    "installs": "npm install && npm run install-user && npm run install-account",
    "install-user": "cd UserApp && npm install",
    "install-account": "cd AccountService && npm install",
    "dev": "concurrently --kill-others \"npm run user\" \"npm run account\"",
    "user": "cd UserApp && npm run dev",
    "account": "cd AccountService && npm run dev"
  },
  "devDependencies": {
    "concurrently": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jorahty/baskin.git"
  },
  "bugs": {
    "url": "https://github.com/jorahty/baskin/issues"
  },
  "homepage": "https://github.com/jorahty/baskin#readme"
}
